

void setup() {
  size (320, 400);
}

void quadros(int h, int v) {
  background(2);
  fill(255);
  //int y=75;
  for (int i=0; i<v; i++) {
    rect(10+70*i, 10+70*i, 60, 80);
    for (int j=0; j<h; j++) {
      //if (h <=2){
      rect(10+70*v+90*j, 10+70*v+90*j, 80, -60);}
      //if(h>2){
     //  rect(10+70*v+90*j, y, 80, -60);
     // }
   // }
  }
}

void draw() {
  quadros(2, 2);
}
