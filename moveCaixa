float i = 0;
void setup() {
  size(600, 600);
}
void draw() {

  MoveCaixa(0, 0, 300, 300, 100);
}
void MoveCaixa(float x1, float y1, float x2, float y2, float n) {//n= algo como a velocidade
  if (i <= 1) {
    background(0);
    rect(x1+i * x2, y1+i*y2, 10, 10);
    i = i + (1.0/n);
  }
  println(mouseX, mouseY);//para a verificação dasd posiçoes
}
